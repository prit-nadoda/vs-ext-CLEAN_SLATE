{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Launch Extension",
		"type": "extensionHost",
		"request": "launch",
		"args": [
		  "--extensionDevelopmentPath=${workspaceFolder}"
		],
		"outFiles": [
		  "${workspaceFolder}/dist/**/*.js"
		],
		"preLaunchTask": "npm: watch",
		"postDebugTask": "npm: watch:esbuild",
		"sourceMaps": true,
		"trace": true
	  }
	]
  }
  